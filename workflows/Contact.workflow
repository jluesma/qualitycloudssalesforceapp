        <workflowTimeTriggers>
            <actions>
                <name>X96hrs_Contact_status_open_AMS</name>
                <type>Alert</type>
            </actions>
            <timeLength>96</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
