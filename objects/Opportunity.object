    <validationRules>
        <fullName>ACV_mgmt_cannot_be_changed_if_ACV_is_0</fullName>
        <active>true</active>
        <description>ACV mgmt commit field (on Opps) cannot be updated/inserted/changed if the ACV field is 0 or empty</description>
        <errorConditionFormula>AND
(
  $User.ProfileId  =  &quot;00ei0000001lugf&quot; ,
 OR ( Annual_contract_value__c = 0.00 , 
 ISBLANK(Annual_contract_value__c ) ) , 
 OR ( ISCHANGED( LIC_FYM_mgmt_commit__c ) ,
  NOT( ISBLANK(LIC_FYM_mgmt_commit__c ) ) )
)</errorConditionFormula>
        <errorMessage>ACV mgmt commit field cannot be updated if the ACV field is 0 or empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_date_validation_for_Pending_opps</fullName>
        <active>false</active>
        <description>Close date can&apos;t be updated for the closed/pending opps</description>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot; 
,$Profile.Name &lt;&gt; &quot;Tricentis Sales Operations&quot;,ISPICKVAL(StageName, &quot;6 Closed/Pending&quot;),ISCHANGED(CloseDate) )</errorConditionFormula>
        <errorMessage>Close date can&apos;t be updated for the closed/pending opps.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Tricentis Sales Operations&quot;,  
ISPICKVAL(StageName, &quot;6 Closed/Won&quot;),
NOT( ( ISPICKVAL( PRIORVALUE(StageName), &quot;6 Closed/Won&quot;))))</errorConditionFormula>
        <errorMessage>You can&apos;t change the Stage to &quot;Closed/Won&quot; .Please set the stage to &quot;Closed/Pending&quot; and reach out to your Admin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_opportunity_cannot_be_edited</fullName>
        <active>true</active>
        <description>Closed Opportunity cannot be edited other than system Admins.</description>
        <errorConditionFormula>AND( ISPICKVAL( PRIORVALUE(StageName), &quot;6 Closed/Won&quot;),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Tricentis Sales Operations&quot;)</errorConditionFormula>
        <errorMessage>Closed Opportunities cannot be edited. Please contact your Admin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_reason_required</fullName>
        <active>true</active>
        <description>Vertiba 3/10/14 User must enter Closed Reason in Debrief Information section when deal is closed</description>
        <errorConditionFormula>(ISPICKVAL(  StageName  ,&quot;6 Closed/Lost&quot;)  || ISPICKVAL(  StageName  ,&quot;6 Closed/Pending&quot;)) &amp;&amp; ISBLANK( Tri_Close_Reason__c )</errorConditionFormula>
        <errorDisplayField>Tri_Close_Reason__c</errorDisplayField>
        <errorMessage>Closed Reason required when opportunity is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_Commission</fullName>
        <active>true</active>
        <description>custom commission cannot be edited unless &quot;Custom&quot; value is chosen.</description>
        <errorConditionFormula>OR(
AND( OR( ISCHANGED( Custom_Commission__c ), ISNEW(), ISCHANGED( Partner_Commission__c )  ), NOT( ISBLANK(Custom_Commission__c )), TEXT( Partner_Commission__c ) &lt;&gt; &quot;Custom&quot; ),
AND(ISPICKVAL( Partner_Commission__c, &quot;Custom&quot;), ISBLANK( Custom_Commission__c  ) )
)</errorConditionFormula>
        <errorMessage>Custom Commission % needs to be provided when &quot;Custom &quot; is selected on &quot;Partner Commission %&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enable_Geo_Access_to_Channel_Global_Role</fullName>
        <active>true</active>
        <description>&quot;Channel Global&quot; Role users will have access to edit the new Geo field along with the profiles
&quot;Tricentis Sales Operations&quot; and &quot;System Administrator&quot;.</description>
        <errorConditionFormula>AND( NOT( BypassValidationForProcessBuilder__c )=TRUE, 
OR( 
AND( 
NOT(ISBLANK(TEXT(Geoo__c))),ISNEW()),ISCHANGED(Geoo__c)), 
NOT( 
OR(($UserRole.Name = &quot;Channel Global&quot;), 
($Profile.Name = &quot;Tricentis Sales Operations&quot;),($Profile.Name = &quot;System Administrator&quot;)) 
) 
)</errorConditionFormula>
        <errorMessage>new Geo field cannot be edited</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Forcast</fullName>
        <active>true</active>
        <description>Lock Forcast field in opportunities, after the have been closed won/lost.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, $Profile.Name &lt;&gt; &quot;Tricentis Sales Operations&quot;,
 OR (
     ISPICKVAL(StageName, &quot;6 Closed Won&quot;),
     ISPICKVAL(StageName, &quot;6 Closed/Lost&quot;)),
ISCHANGED(Tri_Forecast__c)
)</errorConditionFormula>
        <errorDisplayField>Tri_Forecast__c</errorDisplayField>
        <errorMessage>This field cannot be changed anymore.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_field_after_first_entry</fullName>
        <active>false</active>
        <description>This rule should prevent the Lead source field (opportunity) from being updated. 
Requested by Franz Fuchsberger on March 29th 2017 - jassa</description>
        <errorConditionFormula>AND(
    NOT(ISBLANK(PRIORVALUE(LeadSource))),
    ISCHANGED(LeadSource),
    $Profile.Id&lt;&gt;&quot;00ei00000013niz&quot;,
    $Profile.Id&lt;&gt;&quot;00ei0000001luLc&quot;
)</errorConditionFormula>
        <errorDisplayField>LeadSource</errorDisplayField>
        <errorMessage>This field can not be updated if it already has a value. If you need this field be updated please contact your Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_needs_ACV_or_PSO_Amount</fullName>
        <active>false</active>
        <description>No Opportunities with 0 ACV and 0 PSO Amount (&gt;0 total) allowed.</description>
        <errorConditionFormula>(BLANKVALUE( Annual_contract_value__c , 0) &lt;=0  &amp;&amp; BLANKVALUE( PSO_Amount__c , 0)&lt;=0)</errorConditionFormula>
        <errorDisplayField>Annual_contract_value__c</errorDisplayField>
        <errorMessage>Please put in ACV or PSO Amount. For early stage Opportunities with unknown size please use region based default values - see ACV field tooltip)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_close_reason_other</fullName>
        <active>true</active>
        <description>A closed Opportunity with close reason &apos;won/lost - other&apos; needs additional debrief comments. Not applicable for Service only Opps</description>
        <errorConditionFormula>OR(AND(Annual_contract_value__c  &gt; 0,AND(OR(INCLUDES(Tri_Close_Reason__c, &quot;Won - Other Reason&quot;), INCLUDES(Tri_Close_Reason__c, &quot;Lost - Other Reason&quot;)), ISBLANK(Tri_Debrief_Comments__c))), AND( ISPICKVAL(StageName, &quot;6 Closed/Lost&quot;), ISBLANK( Tri_Debrief_Comments__c )  ))</errorConditionFormula>
        <errorDisplayField>Tri_Debrief_Comments__c</errorDisplayField>
        <errorMessage>Please provide additional information in debrief comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subscription_license_needs_term</fullName>
        <active>true</active>
        <description>Subscription license needs term</description>
        <errorConditionFormula>AND(ISPICKVAL(Subscription_Term__c, &quot;&quot;),ISPICKVAL (License_Type__c , &quot;Subscription&quot;))</errorConditionFormula>
        <errorDisplayField>Subscription_Term__c</errorDisplayField>
        <errorMessage>Please select a subscription term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_App_Int_Tech_Tosca</fullName>
        <active>true</active>
        <description>If a Multi picklist has a value of Others and Text Field &quot;Others App/Int/Tech/Tosca&quot; is empty then an error message is sent.</description>
        <errorConditionFormula>OR(AND( INCLUDES( Applications__c ,&quot;Others&quot;) , ISBLANK( Others_Applications__c )),AND( INCLUDES( Integrations__c , &quot;Others&quot;) , ISBLANK( Others_Integrations__c ) ),AND( INCLUDES( Technologies__c ,&quot;Others&quot;), ISBLANK( Others_Technologies__c )), AND( INCLUDES( Tosca_Products__c ,&quot;Others&quot;), ISBLANK( Others_ToscaProducts__c )  ) )</errorConditionFormula>
        <errorMessage>Please specify Others. Indicate the App/Tech/Integration/Toscaproduct missing in the picklist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Why_Commit_Now_for_managers</fullName>
        <active>true</active>
        <description>If Managers enter a value in ACV mgmnt commit , then also enter Why commit now</description>
        <errorConditionFormula>AND( 
OR( 
$Profile.Name = &quot;Depricated_Mgmt Profile incl. Avention&quot;, 
$Profile.Name = &quot;Tricentis Analyst&quot;, 
$Profile.Name = &quot;Tricentis GSI Mgmnt&quot;, 
$Profile.Name = &quot;Tricentis Mgmnt Profile&quot;
), 
NOT(ISBLANK(LIC_FYM_mgmt_commit__c )), 
ISBLANK( Why_commit_now__c ) 
)</errorConditionFormula>
        <errorMessage>Please enter text in &quot;Why Commit Now&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>lock_closed_Opps</fullName>
        <active>true</active>
        <description>As soon as an Opportunity is put to Stage 6 (won or lost), the Stage cannot be changed anymore,; the Opp cannot be re-opened (Admins can re-open) 20161031 jassa</description>
        <errorConditionFormula>AND( 
ISCHANGED( IsClosed), 
PRIORVALUE( IsClosed), 
NOT( IsClosed), 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Tricentis Sales Operations&quot;)</errorConditionFormula>
        <errorMessage>Closed Opportunities cannot be re-opened. Ask your Administrator to re-open this Opportunity for you.</errorMessage>
    </validationRules>
