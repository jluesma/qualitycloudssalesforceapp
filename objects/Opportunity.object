    <fields>
        <fullName>QTD_Current_and_Previous_FY__c</fullName>
        <externalId>false</externalId>
        <formula>IF
(
                OR(
                
                AND(
                                (MONTH(TODAY())=1 || MONTH(TODAY())=2 || MONTH(TODAY())=3), 
                                OR(
                                Date(YEAR(TODAY()),1,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY()),MONTH(TODAY()),DAY(TODAY())),
                                Date(YEAR(TODAY())-1,1,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY())-1,MONTH(TODAY()),DAY(TODAY()))                               )
                ),
                AND(
                                (MONTH(TODAY())=4 || MONTH(TODAY())=5 || MONTH(TODAY())=6), 
                                OR(
                                Date(YEAR(TODAY()),4,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY()),MONTH(TODAY()),DAY(TODAY())),
                                Date(YEAR(TODAY())-1,4,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY())-1,MONTH(TODAY()),DAY(TODAY()))
                                )
                ),
                AND(
                                (MONTH(TODAY())=7 || MONTH(TODAY())=8 || MONTH(TODAY())=9), 
                                OR(
                                Date(YEAR(TODAY()),7,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY()),MONTH(TODAY()),DAY(TODAY())),
                                Date(YEAR(TODAY())-1,7,1) &lt;= CloseDate &amp;&amp; CloseDate &lt;= Date(YEAR(TODAY())-1,MONTH(TODAY()),DAY(TODAY()))
                                )
                ),
                AND(
